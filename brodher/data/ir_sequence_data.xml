<odoo>
    <data noupdate="1">

        <!-- Sequence untuk Artikel -->
        <record id="seq_article_number" model="ir.sequence">
            <field name="name">Article Number Sequence</field>
            <field name="code">article.number.sequence</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>

        <!-- Sequence untuk PIST -->
        <record id="seq_pist_number" model="ir.sequence">
            <field name="name">PIST Number Sequence</field>
            <field name="code">pist.number.sequence</field>
            <field name="prefix"></field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
        </record>

    </data>
    <data noupdate="1">
        <record id="seq_customer_code" model="ir.sequence">
            <name>Customer Code Sequence</name>
            <code>customer.code.sequence</code>
            <padding>7</padding>
            <number_next>1</number_next>
            <number_increment>1</number_increment>
        </record>

        <record id="seq_supplier_code" model="ir.sequence">
            <name>Supplier Code Sequence</name>
            <code>supplier.code.sequence</code>
            <padding>4</padding>
            <number_next>1</number_next>
            <number_increment>1</number_increment>
        </record>
    </data>
</odoo>
